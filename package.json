{
  "name": "fiver",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
  "install-server": "npm install --prefix api",
  "install-client": "yarn --cwd client install",
  "install": "npm-run-all --sequential install-server install-client",
  "server": "npm run start --prefix api",
  "client": "yarn --cwd client run dev",
  "build":"yarn --cwd client run build && npm start --prefix api",
  "watch": "npm-run-all --parallel server client",
  "test": "npm run test --prefix api && yarn --cwd client run test"
},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
